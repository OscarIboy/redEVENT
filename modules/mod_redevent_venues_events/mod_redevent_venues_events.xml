<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install SYSTEM "http://www.joomla.org/xml/dtd/1.5/module-install.dtd">
<install type="module" version="1.5.0" method="upgrade">
	<name>MOD_REDEVENT_VENUES_EVENTS</name>
	<creationDate>Apr 16, 2010</creationDate>
	<author>Redweb.dk</author>
	<authorEmail>email@redweb.dk</authorEmail>
	<authorUrl>www.redweb.dk</authorUrl>
	<copyright>(c) 2008 - 2010 Redweb.dk</copyright>
	<license>GNU/GPL</license>
	<version>2.0 beta 9.1</version>
	<description>MOD_REDEVENT_VENUES_EVENTS_DESC</description>

	<files>
		<filename module="mod_redevent_venues_events">mod_redevent_venues_events.php</filename>
		<filename>index.html</filename>
		<filename>helper.php</filename>
		<filename>mod_redevent_venues_events.css</filename>
		<filename>tmpl/default.php</filename>
		<filename>tmpl/index.html</filename>
	</files>

	<languages>
		<language tag="en-GB">languages/en-GB.mod_redevent_venues_events.ini</language>
	</languages>

	<params>
		<param name="cuttitle" type="text" default="18" label="MOD_REDEVENT_VENUES_EVENTS_TITLE_LENGTH" description="MOD_REDEVENT_VENUES_EVENTS_TITLE_LENGTH_DESC" />
		<param name="catid" type="text" default="" label="MOD_REDEVENT_VENUES_EVENTS_CATEGORY_ID" description="MOD_REDEVENT_VENUES_EVENTS_CATEGORY_ID_DESC" />
		<param name="venid" type="text" default="" label="MOD_REDEVENT_VENUES_EVENTS_VENUE_ID" description="MOD_REDEVENT_VENUES_EVENTS_VENUE_DESC" />
		<param name="moduleclass_sfx" type="text" default="" label="MODULE CLASS SUFFIX" description="MODULE CLASS SUFFIX DESC" />
	</params>

	<params group="advanced">
		<param name="cache" type="list" default="1" label="CACHING" description="CACHING DESC">
			<option value="1">USE GLOBAL</option>
			<option value="0">NO CACHING</option>
		</param>
		<param name="cache_time" type="text" default="900" label="CACHE TIME" description="CACHE TIME DESC" />
	</params>

</install>